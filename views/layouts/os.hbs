                </div>

              <!--<img src="/img/greenskull.gif" class="img-fluid d-flex mx-auto mt-5" href="#" data-toggle="tooltip" title="hello, world" target="_blank" onclick="randomSite();">-->
            </div>
            </div>
        </div>
</div>

      <div class="row">
            {{#if error}}
            <div class="alert alert-warning alert-dismissible fade show noti" role="alert">
              <strong>Whoops!</strong> {{error}}
              <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                ×
              </button>
            </div>
            {{/if}}

            {{#if success}}
            <div class="alert alert-success alert-dismissible fade show noti" role="alert">
              <strong>Yay!</strong> {{success}}
              <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                ×
              </button>
            </div>
            {{/if}}

            {{#if notice}}
            <div class="alert alert-info alert-dismissible fade show noti" role="alert">
              <strong>Oh!</strong> {{notice}}
              <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                ×
              </button>
            </div>
            {{/if}}
        </div>
      </div>

  </div> <!-- closes this column-->



